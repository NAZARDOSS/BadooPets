-- drop database test; 
create database test;

use test;

CREATE TABLE dog (
    id INT PRIMARY KEY AUTO_INCREMENT,
    name VARCHAR(20),
    age INT,
    image_url VARCHAR(1056), -- Изменено на VARCHAR
    description VARCHAR(256),
    price INT,
    type_ ENUM('dog', 'cat', 'bird'),
    gender ENUM('man', 'woman')
);